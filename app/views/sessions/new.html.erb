<div class="row-fluid">
  <div class="span4"></div>
  <div class="span4">

<legend>Login</legend>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] , class: "input-block-level" %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password , params[:password], class: "input-block-level" %>
  </div>
  <div class="field">
    <%= check_box_tag :remember_me, 1, params[:remember_me] %>
    <%= label_tag :remember_me %>
  </div>
  <div class="actions"><%= submit_tag "Log in" ,class: "btn btn-default btn-block"%></div>
<% end %>

</div>